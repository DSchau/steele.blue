---
layout: null
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>Matt Steele</title>
  <link href="http://www.matthew-steele.com"/>
  <link type="application/atom+xml" rel="self" href="http://www.matthew-steele.com/feed/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://www.matthew-steele.com</id>
  <author>
    <name>Matt Steele</name>
    <email>orphum@gmail.com</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>http://www.matthew-steele.com{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://www.matthew-steele.com{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>Matt Steele</name>
      <email>orphum@gmail.com</email>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>
